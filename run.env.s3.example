# Project Configuration
PROJECT=scene

# Skip stages (set to 1 to skip)
SKIP_EXTRACT=1
SKIP_SFM=0
SKIP_DEPTH=0
SKIP_TRAIN=0

# S3 Configuration
S3_INPUT_BUCKET=s3://your-bucket/path/to/input/images
S3_OUTPUT_BUCKET=s3://your-bucket/path/to/output
AWS_REGION=us-east-1

# AWS Credentials (required for S3 access)
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Optional: Video extraction settings
# VIDEO_URL=https://example.com/video.mp4
# NUM_IMAGES=120

# Optional: Depth checkpoint URL
# DEPTH_CKPT_URL=https://huggingface.co/depth-anything/Depth-Anything-V2-Large/resolve/main/depth_anything_v2_vitl.pth
